{
  "name": "technosham-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd ..\\backend && python manage.py runserver 0.0.0.0:8000",
    "dev:backend:venv": "cd ..\\backend && ..\\.venv\\Scripts\\python.exe manage.py runserver 0.0.0.0:8000",
    "dev:all": "concurrently -n FRONTEND,BACKEND -c magenta,cyan \"npm run dev\" \"cd ..\\backend && python manage.py runserver 0.0.0.0:8000\"",
    "dev:all:venv": "concurrently -n FRONTEND,BACKEND -c magenta,cyan \"npm run dev\" \"cd ..\\backend && ..\\.venv\\Scripts\\python.exe manage.py runserver 0.0.0.0:8000\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
  "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^8.2.2",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.2",
    "vitest": "^2.1.1"
  }
}
